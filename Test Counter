	public class CounterTest {

			public static void main(String[] args) {
			
				
				
				Counter venueCounter = new Counter();
				int rValue=venueCounter.getValue();
				

				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				rValue=venueCounter.getValue();
				
				
				venueCounter.unclick();
				venueCounter.unclick();
				venueCounter.unclick();
				venueCounter.unclick();
				venueCounter.unclick();
				rValue=venueCounter.getValue();
				System.out.println("Expected: 5");
				System.out.println("Actual: " + rValue);
				
				venueCounter.reset();
				int mValue = venueCounter.getValue();
				venueCounter.click();
				venueCounter.click();
				venueCounter.click();
				mValue=venueCounter.getValue();
				venueCounter.unclick();
				venueCounter.unclick();
				venueCounter.unclick();
				venueCounter.unclick();
				mValue=venueCounter.getValue();
				System.out.println("Expected: -1");
				System.out.println("Actual: "+mValue);
			}
		}
	
